<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="405.0" prefWidth="614.0"
            style="-fx-background-color: #faebc3;" xmlns="http://javafx.com/javafx/16"
            fx:controller="com.russun.gis.TableController">
    <ScrollPane prefHeight="405.0" prefWidth="486.0">
        <TableView fx:id="shapesTableView" prefHeight="399.0" prefWidth="478.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </ScrollPane>
    <Button layoutX="500.0" layoutY="214.0" mnemonicParsing="false" onAction="#onClickDeleteBtn" prefWidth="100"
            text="Удалить"/>
    <Button layoutX="500.0" layoutY="263.0" mnemonicParsing="false" onAction="#onClickEditBtn" prefWidth="100"
            text="Редактировать"/>
    <Button layoutX="500.0" layoutY="314.0" mnemonicParsing="false" onAction="#onClickAddBtn" prefWidth="100"
            text="Добавить"/>
    <Button layoutX="500.0" layoutY="365.0" mnemonicParsing="false" onAction="#onClickCloseBtn" prefWidth="100"
            text="Закрыть"/>
</AnchorPane>
